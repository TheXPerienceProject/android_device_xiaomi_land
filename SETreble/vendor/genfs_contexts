genfscon proc /sys/kernel/sched_freq_dec_notify      u:object_r:device_proc_kernel_sched:s0
genfscon proc /sys/kernel/sched_freq_inc_notify      u:object_r:device_proc_kernel_sched:s0
genfscon proc /sys/kernel/sched_init_task_load       u:object_r:device_proc_kernel_sched:s0
genfscon proc /sys/kernel/sched_migration_cost_ns    u:object_r:device_proc_kernel_sched:s0
genfscon proc /sys/kernel/sched_migration_fixup      u:object_r:device_proc_kernel_sched:s0
genfscon proc /sys/kernel/sched_small_task           u:object_r:device_proc_kernel_sched:s0
genfscon proc /sys/kernel/sched_spill_nr_run         u:object_r:device_proc_kernel_sched:s0
genfscon proc /sys/kernel/sched_upmigrate            u:object_r:device_proc_kernel_sched:s0
genfscon proc /sys/kernel/sched_cfs_boost            u:object_r:device_proc_kernel_sched:s0
